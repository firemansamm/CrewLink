{
  "name": "crewlink",
  "version": "1.1.6-mod",
  "license": "GPL-3.0-or-later",
  "description": "Free, open, Among Us proximity voice chat",
  "funding": {
    "type": "individual",
    "url": "https://paypal.me/ottomated"
  },
  "author": {
    "name": "Ottomated",
    "email": "otto@ottomated.net",
    "url": "https://ottomated.net"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint src",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder --win --x64 --publish=always",
    "dist-mac": "yarn compile && electron-builder --mac --x64 --publish=always",
    "dist-linux": "yarn compile && electron-builder --linux --x64 --publish=always",
    "dist-all": "yarn compile && electron-builder -mwl"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.11",
    "ajv": "^6.12.6",
    "amongus-protocol": "file:amongus-protocol",
    "analyser-frequency-average": "^1.0.0",
    "axios": "^0.21.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-spawn": "^7.0.3",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "electron-window-state": "^5.0.3",
    "iohook": "^0.7.2",
    "jsondiffpatch": "^0.4.1",
    "jsonfile": "^6.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-spinners-kit": "^1.9.1",
    "react-tooltip-lite": "^1.12.0",
    "simple-peer": "^9.8.0",
    "socket.io-client": "^2.3.1",
    "source-map-support": "^0.5.16",
    "structron": "^0.2.4",
    "ts-interface-checker": "^0.1.13"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.1",
    "@types/color": "^3.0.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "12",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/simple-peer": "^9.6.1",
    "@types/socket.io-client": "^1.4.34",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "electron": "9.3.3",
    "electron-builder": "^22.9.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "ts-interface-builder": "^0.2.2",
    "typescript": "^4.0.3",
    "webpack": "~4.42.1"
  },
  "iohook": {
    "targets": [
      "node-88",
      "electron-80"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "publish": [
      "github"
    ]
  }
}
